    "publish": "changeset publish",
    "docs": "docgen",
    "gen": "plop"
  },
  "author": "Jesse Kelly",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@effect/docgen": "^0.3.6",
