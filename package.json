{
  "$schema": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/schemas/json/package.json",
  "name": "effect-idb",
  "version": "0.0.4",
  "description": "",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "homepage": "",
  "tags": [
    "typescript",
    "effect-ts",
    "indexed-db"
  ],
  "keywords": [
    "typescript",
    "effect-ts",
    "indexed-db"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
